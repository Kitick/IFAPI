{
    "name": "ifcopilot",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "bin": "index.js",
    "scripts": {
        "buildServer": "tsc -p server",
        "postbuildServer": "terser index.js -o index.js -c -m --toplevel",
        "buildSource": "tsc -p source",
        "postbuildSource": "terser public/index.js -o public/index.js -c -m",
        "build": "npm run buildServer",
        "postbuild": "npm run buildSource",
        "precompile": "npm run build",
        "compile": "pkg ."
    },
    "pkg": {
        "targets": [
            "node16-win-x64",
            "node16-linux-x64"
        ],
        "assets": "public/*",
        "outputPath": "executables"
    },
    "author": "Kevin Munson",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "net": "^1.0.2",
        "socket.io": "^4.7.4"
    }
}
